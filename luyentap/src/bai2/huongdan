 n=6071
 temp=n/10^3=6
 ketqua=temp*10;=> ket qua =60
 n = 071

 temp=n/10^2 =0.71
 kết quả = kết quả (60) +0*10= 60

 n = 71
 temp=n/10 = 7.1 // dư 1
 kết quả = kết quả(60)*10 +7 =607

 kết quả = kết quả(607)*10 +1 = 6071

